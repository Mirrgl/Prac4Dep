# SIEM Web Interface Environment Variables

# Required
SIEM_ADMIN_PASSWORD=your_secure_password_here

# Optional - Database
SIEM_DB_HOST=127.0.0.1
SIEM_DB_PORT=8080

# Optional - Web Server
SIEM_WEB_HOST=0.0.0.0
SIEM_WEB_PORT=8000
SIEM_ADMIN_USER=admin

# Optional - Gunicorn (for production)
GUNICORN_WORKERS=4
LOG_LEVEL=info
